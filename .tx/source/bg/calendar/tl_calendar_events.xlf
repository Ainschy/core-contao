        <target>Копиране на събитие</target>
      </trans-unit>
      <trans-unit id="tl_calendar_events.copy.1">
        <source>Duplicate event ID %s</source>
        <target>Копиране на събитие с ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar_events.delete.0">
        <source>Delete event</source>
        <target>Изтриване на събитие</target>
      </trans-unit>
      <trans-unit id="tl_calendar_events.delete.1">
        <source>Delete event ID %s</source>
        <target>Изтриване на събитие с ID %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar_events.editheader.0">
        <source>Edit calendar</source>
        <target>Редактиране на календар</target>
      </trans-unit>
      <trans-unit id="tl_calendar_events.editheader.1">
        <source>Edit the calendar settings</source>
        <target>Редактиране на текущия календар</target>
      </trans-unit>
